/* pages/ciniu/components/juanzeng/juanzeng.wxss */
.juanzeng_content{
  height:100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  padding:0 30rpx;
}
.juanzeng_content .hd view:first-child{
    display: flex;
    justify-content: space-between;
    align-items: center
}



.title .name{
  font-size:24rpx;
  color:#999999;
}
.title .shuoming{
   width:100rpx;
  height:32rpx;
  font-size:20rpx;
  color:#fff;
  background-color: #82c039;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx
}


.money{
  display: flex;
  margin-top:20rpx;
  border-bottom:1px solid #d0d0d0;
  align-items: center
}
.money label{
  font-size:36rpx;
  color:#87421a;
  margin-right:10rpx;
}
.money input{
  flex:1;
  font-size:36rpx;
  color:#87421a
}

.bd{
  display: flex;
  justify-content: space-between
}

.bd .pic{
  width:230rpx;
  height:160rpx;
  background-color: #faf6f6;
  border-radius: 10rpx;
  overflow: hidden;
  background-image: url('data:image/png;base64,');
  background-size: 40rpx 40rpx;
  background-position: center center;
  background-repeat: no-repeat
}

.upload_img{
   width:230rpx;
  height:160rpx;
}


.bd .sub{
  width:230rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between
}
.bd .sub .tip{
  font-size:20rpx;
  color:#999999
}

.ft{
  font-size:24rpx;
  color:#35ae02;
  text-align: center
}

.shuoming_content{
  font-size: 24rpx;
  color: #666;
  display: flex;
  flex-direction: column;
  position: relative;
}

.back_juankuan{
  position: absolute;
  right:0;
  top:0;
  z-index: 10;
   width:100rpx;
  height:32rpx;
  font-size:20rpx;
  color:#fff;
  background-color: #82c039;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
  margin-bottom:10rpx;
  margin-top:20rpx;
  align-self: flex-end;
  margin-right:20rpx;
}

.row {
  margin-bottom: 20rpx;
}

.row .hd {
  margin-bottom: 10rpx;
}

.complete{
  height:100%;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size:24rpx;
  color:#666
}

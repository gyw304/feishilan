.xinyuanlist-content{
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  padding:20rpx 0

}
.items{
  border-left:1px solid #d0d0d0
}

.item{
  display: flex;
  align-items: center;
  margin-left:-14rpx;
}

.item_content{
  width:435rpx;
  max-height:75rpx;
  background-color: #faf6f6;
  border-radius: 10rpx;
  border-bottom:10rpx solid #f2eeee;
  display: flex;
  align-items: flex-start;
  color:#87421a;
  font-size:26rpx;
  margin-bottom:15rpx;
  justify-content: space-between;
  padding:20rpx 0
  
}

.item_content .name{
  margin-left:20rpx;
}

.item_content .date{
  color:#999999;
  font-size:20rpx;
  margin-right:20rpx;
  white-space: nowrap;
  margin-left:20rpx;
}


.item .ico{
  background-color: #fff;
  width:10rpx;
  height:10rpx;
  border:10rpx solid #ff1f1b;
  border-radius: 100%;
  margin-top:-20rpx;
  margin-right:15rpx
}